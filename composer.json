{
  "name": "simple-license/vendor-sdk",
  "description": "PHP SDK for WooCommerce vendors selling licenses via Simple License System",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Simple License System",
      "email": "support@simplelicensesystem.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.5",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "infection/infection": "^0.27",
    "fakerphp/faker": "^1.23",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "SimpleLicense\\Vendor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SimpleLicense\\Vendor\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

